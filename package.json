{
  "name": "Frontend-Express",
  "version": "0.0.0",
  "description": "The frontend of the SecondString sports statistic website",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-server": "node node_modules/typescript/bin/tsc -p src/server/tsconfig.json",
    "build-web": "node node_modules/webpack/bin/webpack --config src/client/webpack.dev.js --mode development",
    "build-web-prod": "node node_modules/webpack/bin/webpack --config src/client/webpack.prod.js --mode production",
    "watch": "node node_modules/typescript/bin/tsc -w -p src/server/tsconfig.json & node node_modules/webpack/bin/webpack --config src/client/webpack.dev.js --display-modules --mode development --watch & node node_modules/nodemon/bin/nodemon dist/server/main.js",
    "start": "node dist/server/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Second-String/FrontEnd-Express.git"
  },
  "author": "SecondString",
  "license": "MIT",
  "homepage": "",
  "dependencies": {
    "@google-cloud/speech": "^1.5.0",
    "@types/compression": "0.0.36",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/node-fetch": "^2.1.1",
    "@types/webspeechapi": "0.0.29",
    "@types/ws": "^5.1.2",
    "@types/xml2js": "^0.4.3",
    "audiobuffer-to-wav": "^1.0.0",
    "compression": "^1.7.2",
    "css-loader": "^0.28.11",
    "dialogflow": "^0.5.0",
    "dotenv": "^6.0.0",
    "encode-wav": "^0.1.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fetch": "^1.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "lit-html": "^0.10.2",
    "node-fetch": "^2.1.2",
    "nodemon": "^1.17.5",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.4.1",
    "typescript": "^2.9.2",
    "wav-encoder": "^1.3.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-merge": "^4.1.3",
